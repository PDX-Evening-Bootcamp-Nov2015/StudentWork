console.log ("Zero is", 4+4-4-4)
console.log ("One is", 4-4+4/4)
console.log ("Two is", 4/4+4/4)
console.log ("Three is", (4+4+4)/4)
console.log ("Four is", Math.pow((4/4), 4*4);)
console.log ("Five is", Math.pow((4/4), 4+4);)
console.log ("Six is", ((4+4)/4)+4)
console.log ("Seven is", 4+4-4/4)
console.log ("Eight is", 4+4+4-4)
console.log ("Nine is", 4+4+4/4)
